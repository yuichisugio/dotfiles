{
	"workbench.iconTheme": "vscode-icons",
	"workbench.startupEditor": "none",
	"files.autoSave": "afterDelay",
	"editor.tabSize": 2,
	"prettier.singleQuote": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.wordWrap": "bounded",
	"prettier.printWidth": 120,
	"prettier.trailingComma": "none",
	"liveServer.settings.donotShowInfoMsg": true,
	"notebook.formatOnSave.enabled": true,
	"github.copilot.editor.enableAutoCompletions": true,
	"git.autofetch": true,
	"terminal.integrated.defaultProfile.osx": "zsh",
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"window.restoreWindows": "preserve",
	"editor.linkedEditing": true,
	"git.openRepositoryInParentFolders": "always",
	"vsicons.dontShowNewVersionMessage": true,
	"editor.minimap.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"eslint.probe": [
		"astro",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"mdx",
		"vue",
		"markdown",
		"json",
		"jsonc",
		"react"
	],
	"prettier.enableDebugLogs": true,
	"prettier.jsxSingleQuote": true,
	"prettier.singleAttributePerLine": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": true,
	"files.trimFinalNewlines": true,
	"files.insertFinalNewline": true,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.reduceMotion": "on",
	"vs-kubernetes": {
		"vscode-kubernetes.helm-path-mac": "/Users/sugioyuuichi/.vs-kubernetes/tools/helm/darwin-amd64/helm",
		"vscode-kubernetes.kubectl-path-mac": "/Users/yuichi.sugio/.vs-kubernetes/tools/kubectl/kubectl"
	},
	"tabnine.experimentalAutoImports": true,
	"editor.wordWrapColumn": 100,
	"screencastMode.keyboardOptions": {
		"showCommandGroups": true,
		"showCommands": false
	},
	"geminicodeassist.project": "",
	"workbench.colorTheme": "Quiet Light",
	"cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
	"cursor.cpp.enablePartialAccepts": true,
	"cursor.composer.collapsePaneInputBoxPills": true,
	"cursor.composer.shouldAllowCustomModes": true,
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"cursor.diffs.useCharacterLevelDiffs": true,
	"cursor.terminal.usePreviewBox": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"explorer.fileNesting.patterns": {
		"*ts": "${capture}.*.ts",
		"*.ts": "${capture}.*.ts",
		"*.tsx": "${capture}.*.tsx"
	},
	"explorer.fileNesting.enabled": true,
	"terminal.integrated.defaultProfile.linux": "zsh",
	"terminal.integrated.scrollback": 20000,
	"explorer.confirmDelete": false,
	"application.shellEnvironmentResolutionTimeout": 30,
	"explorer.confirmDragAndDrop": false,
	"workbench.editor.enablePreview": false,
	"cursor.windowSwitcher.sidebarHoverCollapsed": true,
	"[shellscript]": {
		"editor.defaultFormatter": "mkhl.shfmt"
	},
	"shellcheck.customArgs": ["-x"],
	"[dockercompose]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "advanced",
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"[github-actions-workflow]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"cursor.agent_layout_browser_beta_setting": true,
	"cursor.enable_agent_window_setting": true,
	"cursor.rpcFileLogger.enabled": true
}
